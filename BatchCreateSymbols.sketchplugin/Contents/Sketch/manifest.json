{
  "name": "Batch Create Symbols",
  "description": "Plugin to create an individual symbol out of every selected layer",
  "author": "Paul Demers",
  "homepage": "https://raw.githubusercontent.com/michelve/sketch-batch-create-symbols/master/appcast.xml",
  "version": "2.1.1",
  "identifier": "com.example.sketch.batch-create-symbols",
  "icon" : "icon.png",
  "commands": [
    {
      "name": "Use Layer Names",
      "identifier": "create-symbols",
      "shortcut": "ctrl opt shift b",
      "script": "batch-create-symbols.cocoascript",
      "handler": "createSymbols"
    },
    {
      "name": "Use Layer Names With Prefix",
      "identifier": "create-prefix-symbols",
      "shortcut": "ctrl opt shift p",
      "script": "batch-create-symbols.cocoascript",
      "handler": "createSymbolsWithPrefix"
    },
    {
      "name": "Use Custom Names",
      "identifier": "create-custom-symbols",
      "shortcut": "ctrl opt shift c",
      "script": "batch-create-symbols.cocoascript",
      "handler": "createCustomSymbols"
    },
    {
      "name": "Use Custom Folders",
      "identifier": "create-custom-folders",
      "shortcut": "ctrl opt shift f",
      "script": "batch-create-symbols.cocoascript",
      "handler": "createCustomSymbolWithFolder"
    }
  ],
  "menu": {
    "title": "Batch Create Symbols",
    "items": [
      "create-symbols",
      "create-prefix-symbols",
      "create-custom-symbols",
      "create-custom-folders"
    ]
  }
}
