{
  "name": "sketch-batch-create-symbols",
  "description": "Quickly create an individual symbol out of every selected layer",
  "author": "Paul Demers",
  "authorEmail": "paul@pdemersjr.com",
  "version": "2.0.0",
  "identifier": "com.example.sketch-batch-create-symbols",
  "appcast": "https://raw.githubusercontent.com/demersdesigns/sketch-batch-create-symbols/master/.appcast.xml",
  "compatibleVersion": ">=49.0",
  "disableCocoaScriptPreprocessor": true,
  "icon": "icon.png",
  "commands": [
    {
      "name": "Use Layer Names",
      "identifier": "create-symbols",
      "shortcut": "ctrl opt shift b",
      "script": "batch-create-symbols.js",
      "handler": "createSymbols"
    },
    {
      "name": "Use Custom Names",
      "identifier": "create-custom-symbols",
      "shortcut": "ctrl opt shift c",
      "script": "batch-create-symbols.js",
      "handler": "createSymbolsCustomName"
    },
    {
      "name": "Use Custom Prefix",
      "identifier": "create-custom-prefix",
      "shortcut": "ctrl opt shift p",
      "script": "batch-create-symbols.js",
      "handler": "createSymbolsCustomPrefix"
    },
    {
      "name": "Use Custom Folders",
      "identifier": "create-custom-folders",
      "shortcut": "ctrl opt shift f",
      "script": "batch-create-symbols.js",
      "handler": "createSymbolsCustomFolder"
    }
  ],
  "menu": {
    "title": "Batch Create Symbols",
    "items": [
      "create-symbols",
      "create-custom-symbols",
      "create-custom-prefix",
      "create-custom-folders"
    ]
  }
}